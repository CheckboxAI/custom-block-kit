{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2016",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "strict": true,
    "sourceMap": true,
    "noEmit": true,
    "outDir": "dist",
    "types": ["jest", "node"]
  },
  "lib": ["es2015"]
}
