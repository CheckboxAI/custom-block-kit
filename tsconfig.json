{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es2016",
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noUncheckedIndexedAccess": true,
        "strict": true,
        "noEmit": true,
        "outDir": "dist"
    },
    "lib": ["es2015"]
}
